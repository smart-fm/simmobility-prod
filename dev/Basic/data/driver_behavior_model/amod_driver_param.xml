	<!--From mitsim:-->
	<!-- param name="CF_parameters_1" value="	0.0916	0.722	0.242	0.682	0.600	#0.825"/-->
	<!-- param name="CF_parameters_2" value="	-0.0278	0.000	0.151	0.804	0.682	#0.802"/-->

